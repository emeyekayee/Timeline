<script>

  <% method = { 'lo' => 'prepend', 'hi' => 'append' }[@inc]  || "html"
     @rsrcs.each do |rsrc|  
       html = render(:partial => 'spancontent',
                     :object => (@blockss[rsrc] || []))
       id   = "#timespan#{rsrc}"                        %>
         $("<%= id %>").<%= method %>( "<%= escape_javascript(html) %>" )
  <% end %>

  <!-- after_update() -->
       window.grid.complete_data_request()
</script>

