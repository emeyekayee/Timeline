
  <% method = "html"                   
     method = "append"  if @inc == "hi"
     method = "prepend" if @inc == "lo"
     
     @rsrcs.each do |rsrc|  
       html = render(:partial => 'spancontent',
                     :object => (@blockss[rsrc] || []))
       id   = "#timespan#{rsrc}"                        %>

       $("<%= id %>").<%= method %>( "<%= escape_javascript(html) %>" )
  <% end %>

     after_update()

